<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/perfilLayuot"
    android:minWidth="25px"
    android:minHeight="25px">
    <include
        layout="@layout/app_bar" />
    <ImageView
        android:src="@drawable/material_background"
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="114.7dp"
        android:layout_marginBottom="0.0dp"
        android:layout_below="@id/app_bar" />
    <android.support.design.widget.FloatingActionButton xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_camera_alt_white_18dp"
        android:layout_marginRight="15.0dp"
        android:layout_alignRight="@id/imageView1"
        android:layout_centerHorizontal="true"
        android:layout_alignBottom="@id/imageView1"
        app:fabSize="mini"
        app:backgroundTint="@android:color/holo_green_light" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_centerInParent="true"
        android:layout_below="@id/imageView1">
        <android.support.design.widget.TextInputLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textInputLayout4"
            android:theme="@style/TextLabel"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp">
            <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="First name"
                android:singleLine="true"
                android:inputType="textPersonName"
                android:id="@+id/txtFirstName" />
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textInputLayout2"
            android:theme="@style/TextLabel"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp">
            <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Last name"
                android:inputType="textPersonName"
                android:singleLine="true"
                android:id="@+id/txtLastname" />
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textInputLayout3"
            android:theme="@style/TextLabel"
            android:layout_marginRight="30dp"
            android:layout_marginLeft="30dp">
            <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Email"
                android:inputType="textEmailAddress"
                android:editable="false"
                android:singleLine="true"
                android:id="@+id/txtEmail" />
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/TextLabel"
            android:id="@+id/textInputLayout5"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp">
            <android.support.design.widget.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Location"
                android:singleLine="true"
                android:id="@+id/txtLocationUser" />
        </android.support.design.widget.TextInputLayout>
        <TextView
            android:text="Genero favorito"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp" />
        <Spinner
            android:layout_width="162.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/spnGenero"
            android:spinnerMode="dialog"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout2"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp">
            <Button
                android:text="Actualizar"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:backgroundTint="@android:color/holo_green_light"
                android:id="@+id/btnUpdate"
                android:textColor="@android:color/background_light"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="5dp" />
            <Button
                android:text="Cancelar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@android:color/holo_green_light"
                android:id="@+id/btnCancelar"
                android:textColor="@android:color/background_light" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>